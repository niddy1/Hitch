<h3>Single's Log In</h3>

<%= form_tag '/sessions', method: :post do %>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <%= submit_tag "Log In" %>
<% end %>

<a href="/hitchers/log_in">Are you a Hitcher? Log in here</a>
